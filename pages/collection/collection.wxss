.select-box{
  border-bottom: 1px solid #e3e3e3;
  margin-bottom: 10rpx;
}
.type-one{
  padding-bottom: 10rpx;
  font-size:32rpx;
}
.type-one text{
  display:inline-block;
  margin: 10rpx 10rpx 0 0;
  padding:10rpx 20rpx;
  border:1px solid #e3e3e3;
  border-radius:5px;
  background-color: #e3e3e3;
}
.select-box .active{
  border-color: #f00;
  color: #f00;
  background-color: #fff;
}

.list-nolike{
  margin-top: 300rpx;
  text-align: center;
  color: #666;
}
.list-nolike view{
  margin: 20rpx auto 30rpx;
}
.list-nolike .iconfont{
  font-size: 60rpx;
}
.list-nolike .click-link{
  color: #4183c4;
  font-size: 28rpx;
  text-decoration: underline;
}

.list-loading,
.list-loadAll{
  margin-top: 20rpx;
  color: #666;
  font-size: 28rpx;
  text-align: center;
}